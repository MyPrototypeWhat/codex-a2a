{
  "name": "codex-a2a",
  "version": "0.1.0",
  "description": "A2A server adapter for OpenAI Codex",
  "keywords": [
    "a2a",
    "agent",
    "codex",
    "json-rpc",
    "openai",
    "rest",
    "server",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MyPrototypeWhat/codex-a2a.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.0",
    "@openai/codex-sdk": "^0.92.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.0",
    "tsdown": "^0.8.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0"
}
